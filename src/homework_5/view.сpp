#include "homework_5/graphical_objects.hpp"
#include "view.hpp"
#include <utility>

namespace homework_5 {

namespace {
std::list<View> views;
}

Canvas::Canvas(Point begin_point, Point end_point)
    : begin_point_(begin_point), end_point_(end_point) {}

void Canvas::AddGraphicalObject(Color color, Point begin_object) {
  objects_.emplace_back(color, begin_object);
}

void Canvas::Draw() {}

void AddText(int32_t x, int32_t y, Color color, std::string_view data) {}
void AddLine(int32_t x, int32_t y, Color color) {}
void AddSquare(int32_t x, int32_t y, Color color, std::list<Line> &lines_) {}
void AddRectangle(int32_t x, int32_t y, Color color, std::list<Line> &lines_) {}

} // namespace homework_5